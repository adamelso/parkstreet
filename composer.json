{
    "name": "adam.elsodaney/parkstreet",
    "description": "Technical test",
    "type": "project",
    "require": {
        "php": ">=7.0",

        "roave/security-advisories": "dev-master",

        "doctrine/orm": "^2.5",
        "guzzlehttp/guzzle": "^6.2",
        "markrogoyski/math-php": "^0.28.0",
        "pimple/pimple": "^3.0",
        "symfony/console": "^3.3"

    },
    "require-dev": {
        "phpspec/phpspec": "^3.3",
        "behat/behat": "^3.3"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Adam Elsodaney",
            "email": "adam.elso@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "config": {
        "bin-dir": "bin"
    },
    "bin": [
        "bin/parkstreet"
    ],
    "autoload": {
        "psr-4": {
            "ParkStreet\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "spec\\ParkStreet\\": "spec"
        }
    }
}
